<Window x:Class="WpfApp3.DeleteUserWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:WpfApp3"
		WindowStartupLocation="CenterScreen"
		mc:Ignorable="d"
		Title="DeleteUserWindow" Height="400" Width="400">
	<Grid>
		<TextBlock Text="Wybierz użytkownika:" TextWrapping="Wrap" HorizontalAlignment="Center" Margin="0,130"/>
		<ComboBox Name="UsersList" Width="120" Height="20">
		</ComboBox>
		<Button Name="Delete" Content="Usuń użytkownika" Width="100" Height="20" Margin="20,130,20,0" Click="Delete_Click"/>
		<Button Name="back" Click="back_Click" Content="Powrót" Height="20" Margin="20,250,20,0" Width="50"/>
	</Grid>
</Window>
